model CheckIn {
  id           String    @id @default(uuid())
  created_at   DateTime  @default(now())
  validated_at DateTime?

  user    User   @relation(fields: [user_id], references: [id])
  user_id String
  gym     Gym    @relation(fields: [gym_id], references: [id])
  gym_id  String

  @@map("check_ins")
}
